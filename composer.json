{
    "name": "ikechukwukalu/sanctumauthstarter",
    "description": "Laravel sanctum auth starter kit",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ikechukwu Kalu"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/ui": "^3.4",
        "laravel/sanctum": "^3.0",
        "laravel/framework": "^9.19"
    },
    "autoload": {
        "psr-4": {
            "Ikechukwukalu\\Sanctumauthstarter\\": "src/",
            "Ikechukwukalu\\Sanctumauthstarter\\Controllers": "src/Controllers"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ikechukwukalu\\Sanctumauthstarter\\SanctumauthstarterServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev"
    }
}
